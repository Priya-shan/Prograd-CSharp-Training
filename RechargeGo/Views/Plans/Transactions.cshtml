@model IEnumerable<Object>
@*
    For more information on enabling MVC for empty projects, visit https://go.microsoft.com/fwlink/?LinkID=397860
*@
@{
    //var joinedList = ViewBag.joinedList as List<CombinedData>;
}
<h2 style="text-align:center">All Transactions</h2>
<br />
<table class="table">
    <thead>
        <tr>
            <th>
                Recharge Id
            </th>
            <th>
               Custome Name
            </th>
            <th>
                Mobile Number
            </th>
            <th>
                Operator
            </th>
            <th>
                Plan Name
            </th>
            <th>
                Amount
            </th>
            <th>
                Date
            </th>
            <th>
                Status
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (CombinedData item in ViewBag.joinedList)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.recharge_id )
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.customer_name)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.mobile_number)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.operator_name)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.category)
                </td>
                <td>
                    ₹@Html.DisplayFor(modelItem => item.amount)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.date)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.transact_status)
                </td>
            </tr>
        }
    </tbody>
</table>